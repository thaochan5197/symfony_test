{% trans_default_domain 'FOSUserBundle' %}

{% if error %}
    <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
{% endif %}


<div id="token" token="{{ csrf_token }}"></div>
{% block javascripts %}
<script >
    const token = document.getElementById("token").getAttribute("token");
    console.log(token);
</script>
{% endblock %}

{% if csrf_token %}
    <input type="hidden" name="_csrf_token" id="abc" value="{{ csrf_token }}" />
{% endif %}

<div id="loginWrapper">

</div>



